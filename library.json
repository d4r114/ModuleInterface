{
  "name": "ModuleInterface",
  "description": "ModuleInterface is an open-source library for simplifying data exchange between devices",
  "keywords": "communication,protocol,synchronization,database,web page,network",
  "license": "Apache-2.0",
  "authors":
  {
    "name": "Fred Larsen",
    "email": "fredilarsen@hotmail.com",
    "maintainer": true
  },
  "repository":
  {
    "type": "git",
    "url": "https://github.com/fredilarsen/ModuleInterface.git"
  },
  "version": "1.1.0",
  "examples": [
    "examples/*/*.ino", 
    "examples/*/*/*.ino",
    "examples/*/*/*/*.ino"
    ],
  "frameworks": "arduino",
  "dependencies": {
      "PJON" : "~8.2.0,~9.1.0",
      "ArduinoJson" : ">=5.0.0"
  },
  "platforms":
  [
    "atmelavr",
    "atmelsam",
    "espressif",
    "teensy"
  ],
  "build" : {
    "flags": "-std=c++11"
  }
}
